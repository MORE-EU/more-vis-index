<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration>

<configuration scan="true">
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <layout class="ch.qos.logback.classic.PatternLayout">
            <Pattern>
                %d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n
            </Pattern>
        </layout>
    </appender>
    <logger name="org.apache.parquet" level="ERROR"/>
    <logger name="org.apache.hadoop" level="ERROR"/>
    <logger name="eu.more2020.visual.util.io.ParquetReader" level="INFO"/>
    <logger name="eu.more2020.visual.util.io.CsvReader.CsvRandomAccessReader" level="INFO"/>
    <logger name="eu.more2020.visual.experiments.Experiments" level="INFO"/>
    <logger name="eu.more2020.visual.experiments.util.QuerySequenceGenerator" level="INFO"/>
    <logger name="c.m.pebble.lexer.TemplateSource" level="INFO"/>
    <root>
        <level>INFO</level>
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>